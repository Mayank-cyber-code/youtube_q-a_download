{
  "manifest_version": 3,
  "name": "YouTube Q&A",
  "version": "1.0",
  "description": "Ask questions about YouTube videos using OpenAI and subtitles.",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
